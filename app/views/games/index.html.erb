<p id="notice"><%= notice %></p>

<h1>Games</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><%= game.name %></td>
        <td><%= game.start_time %></td>

        <td><%= link_to '参加する', new_game_entry_path(game) %></td>
      </tr>
    <% end %>
  </tbody>
</table>


